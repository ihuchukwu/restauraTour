<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<p id="notice"><%= notice %></p>


<%= link_to 'Sign up', new_user_registration_path %>


<% @users.each do |u| %>
      <p><%= u.first_name %></p>
      <p><%= u.last_name %></p>
      <p><%= u.location %></p>
      <p><%= u.email %></p>

<% end %>
